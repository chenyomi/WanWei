/* pages/orderList/orderList.wxss */
@import "../../pages/template/modelTip.wxss";
.padding-30{padding: 0 30rpx;}

.title{font-size: 28rpx;color:#707070;line-height: 80rpx;padding-left: 30rpx}
.order-title{font-size: 26rpx;color:#707070;line-height: 50rpx;padding-left: 30rpx}
.order-price{font-size: 26rpx;color:#707070;line-height: 50rpx;}
.back_fff{background: #fff;}

.add{display: inline-block;height: 40rpx;width: 40rpx;border: 1px solid #e7e7e7;line-height: 40rpx;text-align: center;font-size: 30rpx;color:#e7e7e7;vertical-align: middle}
.del{display: inline-block;height: 40rpx;width: 40rpx;border: 1px solid #e7e7e7;line-height: 40rpx;text-align: center;font-size: 30rpx;color:#e7e7e7;vertical-align: middle}

.input{display: inline-block;min-height: 40rpx;height: 40rpx;width:50rpx;border: 1px solid #e7e7e7;line-height: 30rpx;text-align: center;font-size: 30rpx;color:#a0a0a0;vertical-align: middle;margin:0 10rpx}

.active{border-color: #f76120;color: #f76120}

.button-float{background:#000;color:#fff;text-align:center;line-height:39px;font-size:28rpx;width:100%;border-radius:0;}
.fixed{position: fixed;bottom: 0;left: 0;width: 100%;background: rgba(255,255,255,.9);vertical-align: middle;border-top: 1px solid #f0f0f0}

.sign-up{background: #fff; padding: 0 30rpx; }